<h1><%= @machine.location %> Vending Machine</h1>


  <%=@machine.snacks.each do |snack| %>
  <section id="snacklist-<%=snack.id%>">
    <p>Snack Name: <%=snack.name %></p>
    <p>Price: $<%=snack.price %></p>
  </section>
  <%end %>

<section id= "aveprice">
<p>Average Snack Price: <%= @machine.snacks.ave_price %></p>
</section>
